      "Bash(grep:*)",
      "Bash(rm:*)",
      "Bash(ls:*)",
      "Bash(cp:*)",
      "Bash(git restore:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git count-objects:*)",
      "Bash(git push:*)",
      "WebFetch(domain:www.youtube.com)",
      "WebFetch(domain:youtu.be)",
      "WebFetch(domain:youtu.be)",
      "WebFetch(domain:youtu.be)",
      "Bash(npm run build:*)"
